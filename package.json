{
  "name": "csld-react",
  "description": "CSLD React FE",
  "version": "1.0.0",
  "repository": "https://gitlab.com/lemmingucwcz/csld-react.git",
  "author": "Michal Kara <michal.kara@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "clean": "rimraf node_modules coverage .next dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest",
    "update-snapshots": "jest --update-snapshot",
    "coverage": "jest --coverage",
    "gen:schema": "graphql-codegen && prettier --write --config .prettierrc.js src/graphql/__generated__/*.{ts,tsx}",
    "lint": "eslint {src,pages,server}/**/* --ext .tsx,.ts",
    "pretty": "prettier --write --config .prettierrc.js {src,pages,server}/**/*.{ts,tsx}",
    "format": "yarn lint --fix",
    "verify": "yarn lint && yarn test",
    "analyze": "cross-env BUNDLE_ANALYZE=both yarn build",
    "export": "cross-env NODE_ENV=production next export",
    "storybook": "start-storybook -s ./public/static -p 6006",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
